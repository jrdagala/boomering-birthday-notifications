    "aws-lambda": "^1.0.7",
    "convict": "6.2.4",
    "dotenv": "16.4.7",
    "dynamoose": "4.0.4",
    "express": "5.2.1",
    "http-status": "^2.1.0",
    "ioredis": "5.8.2",
    "moment-timezone": "0.6.0",
    "serverless-http": "3.2.0",
    "uuid": "9.0.1",
    "zod": "4.1.13"
