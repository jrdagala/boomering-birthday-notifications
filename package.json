    "esbuild": "^0.27.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "ioredis-mock": "^8.13.1",
    "jest": "30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "serverless-localstack": "^1.3.1",
